  	<script type="text/javascript">
  	$(document).ready(function(){
			  $.ajax({
				url : "ClasseList",
				type: "GET",
				dataType: 'json',
				success: function(response,textStatus ,jqXHR){
					var tableData = $('#datatable').DataTable();
					for(i=0;i<response.length;i++){
						var btn='<button class="btn btn-primary" id="bouton" onclick="alert("ok")">Test</button>';
						 tableData.row.add( [
						             null,
						             response[i].classeGenerique.cycle.code,
						             response[i].classeGenerique.filiere.code,
						             response[i].classeGenerique.niveau.code,
						             btn,
									 null,
									 null
						             
						         ] ).draw( false );
					}
			    },
			    error: function(response,textStatus ,jqXHR){
			    	console.log("Something really bad happened " + textStatus);
                    //$("#ajaxResponse").html(jqXHR.responseText);
                    alert("Erreur de chargement des classes "+textStatus);
			        }
			        
			});
			  
		});
  	</script>
